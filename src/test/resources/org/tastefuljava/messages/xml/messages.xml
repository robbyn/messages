<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE messages
    PUBLIC '-//tastefuljava.org//Message File 1.0//EN' 'messages.dtd'>
<messages language="en">
    <message name="appNotFound" parameters="app">
        Unknown application: <out value="app"/>.
    </message>
    <message name="choose" parameters="a,b">
        (<choose>
            <when condition="a==0">
                zero
                <choose>
                    <when condition="b==0">
                        zero
                    </when>
                    <when condition="b==1">
                        one
                    </when>
                    <when condition="b==2">
                        two
                    </when>
                    <when condition="b==3">
                        three
                    </when>
                    <otherwise>
                        something else
                    </otherwise>
                </choose>
            </when>
            <when condition="a==1">
                one
            </when>
            <when condition="a==2">
                two
            </when>
            <when condition="a==3">
                three
            </when>
            <otherwise>
                something else
                <if condition="b==4">
                    four
                </if>
            </otherwise>
        </choose>)
    </message>
    <message name="list" parameters="list">
        (<list value="list" variable="elem">
            <out value="elem"/>
        </list>)
    </message>
    <message name="list2" parameters="list">
        (<list value="list" variable="elem">
            <out value="text.enquote(elem)"/>
        </list>)
    </message>
    <message name="list3" parameters="list">
        (<out value='text.join(list,", ")'/>)
    </message>
    <message name="list4" parameters="list">
        (<out value='text.joinEnquote(list,", ")'/>)
    </message>
    <message name="index" parameters="a,i">
        <out value="a[i]"/>
    </message>
    <message name="text" parameters="line1,line2,line3">
        ----    Begin text    ----

        <out value="line1"/>

        <out value="line2"/>

        <out value="line3"/>

        ----    End text    ----
    </message>
</messages>
